#include <stdio.h>
#include <wiringPi.h>
#include <Python.h>

#define MAXLINESIZE 20

int main()
{
	char calendarEntry[MAXLINESIZE];

	Py_SetProgramName(argv[0]);
	Py_Initialize();
	PyRun_SimpleFile("from time import time,ctime\n"
                     "print 'Today is',ctime(time())\n");
	Py_Finalize();
	
	wiringPiSetup();
	
	
}
